<textcomponents> <!-- slide 1-->
<component id="ID_s1_title">
<text><![CDATA[<b>Conversation TCP  </b>]]></text>
</component> <!--animation-->
	<component id="ID_s1_txt01">
	<text><![CDATA[SYN TCP]]></text>
	</component>
	
	<component id="ID_s1_txt02">
	<text><![CDATA[«&nbsp;J'aimerais vous parler.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt03">
	<text><![CDATA[«&nbsp;D'accord, je vous écoute.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt04">
	<text><![CDATA[SYN/ACK TCP]]></text>
	</component>
	
	<component id="ID_s1_txt05">
	<text><![CDATA[ACK TCP]]></text>
	</component>
	
	<component id="ID_s1_txt06">
	<text><![CDATA[«&nbsp;Super, nous avons établi une connexion.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt07">
	<text><![CDATA[Données TCP]]></text>
	</component>
	
	<component id="ID_s1_txt08">
	<text><![CDATA[«&nbsp;J'ai besoin que vous m'envoyiez des données.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt09">
	<text><![CDATA[«&nbsp;Merci, j'ai reçu votre requête.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt10">
	<text><![CDATA[ACK TCP]]></text>
	</component>
	
	<component id="ID_s1_txt11">
	<text><![CDATA[«&nbsp;Voici les données demandées.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt12">
	<text><![CDATA[Données TCP]]></text>
	</component>
	
	<component id="ID_s1_txt13">
	<text><![CDATA[ACK TCP]]></text>
	</component>
	
	<component id="ID_s1_txt14">
	<text><![CDATA[«&nbsp;Merci. J'ai reçu mes données.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt15">
	<text><![CDATA[«&nbsp;J'ai terminé et je n'ai plus de données à envoyer.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt16">
	<text><![CDATA[TCP FIN]]></text>
	</component>
	
	<component id="ID_s1_txt17">
	<text><![CDATA[FIN/ACK TCP]]></text>
	</component>
	
	<component id="ID_s1_txt18">
	<text><![CDATA[«&nbsp;J'ai aussi terminé. Merci.&nbsp;»]]></text>
	</component>

	<component id="ID_s1_txt19">
	<text><![CDATA[«&nbsp;Merci.&nbsp;»]]></text>
	</component>
	
	<component id="ID_s1_txt20">
	<text><![CDATA[ACK TCP]]></text>
	</component> <!-- slide 2-->
